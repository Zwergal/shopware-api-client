{
  "name": "shopware-api-client",
  "version": "0.1.0",
  "description": "node client for shopware api",
  "main": "dist/index.js",
  "author": "Jakub Juszczak <jakub@posteo.de>",
  "license": "MIT",
  "scripts": {
    "start": "gulp",
    "build": "gulp transpile",
    "prepublish": "npm run build",
    "lint": "xo",
    "test": "ava --timeout=30s"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "ava": {
    "failFast": true,
    "files": [
      "test/*.js"
    ]
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "ignores": [
      "shopware-api-cli.js"
    ],
    "rules": {
      "import/no-unassigned-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-unresolved": 0,
      "max-depth": 0,
      "max-params": 0,
      "no-use-before-define": 0,
      "complexity": 0,
      "no-control-regex": 0
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "ava-spec": "^1.1.0",
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.1",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3"
  }
}
